{
X	register u_long btmp;
X	register short int i, count;
X	int 	olen = 0 ;
X
X	while (*ibuf && ilen > 0 )
X	{
X		for (i = count = 0, btmp = 0; i < 3; i++)
X			if (*ibuf && ilen > 0 )
X			{
X				btmp = (btmp << 8) | (u_long)(*ibuf & 0x0ff);
X				ibuf++; ilen-- ;
X				count++;
X			}
X			else
X				btmp = (btmp << 8) | (u_long)0;
X
X		*obuf++ = b64_alphabet[(B64_0 & btmp) >> 18];
X		*obuf++ = b64_alphabet[(B64_1 & btmp) >> 12];
X		*obuf++ = b64_alphabet[count >= 2 ? (B64_2 & btmp) >> 6 : PAD];
X		*obuf++ = b64_alphabet[count == 3 ? (B64_3 & btmp) : PAD];
X		olen += 4 ;
X	}
X
X	*obuf = '\0';
X	return(olen);
}  /* end of string_to_base64(). */
