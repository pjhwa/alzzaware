X		if (++count >= 4)
X		{
X			*obuf++ = (char)((btmp & 0x0ff0000) >> 16);
X			*obuf++ = (char)((btmp & 0x000ff00) >> 8);
X			*obuf++ = (char)(btmp & 0x00000ff);
X			olen +=3 ;
