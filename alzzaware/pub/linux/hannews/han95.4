int base64_to_string(ibuf, obuf, ilen )
char *ibuf, *obuf;
int	ilen ;	/*   maximum ibuf length, it may be shorter	*/
{
X	register u_long btmp = 0;
X	register short int count = 0;
X	int 	olen = 0 ;
X
X	while (*ibuf && ilen > 0 )
X	{
X		if (*ibuf == '+')
X			btmp = (btmp << 6) | 62;
X		else if (*ibuf == '/')
X			btmp = (btmp << 6) | 63;
X		else if (isdigit(*ibuf))
X			btmp = (btmp << 6) | (52 + *ibuf - '0');
X		else if (isupper(*ibuf))
X			btmp = (btmp << 6) | (*ibuf - 'A');
X		else if (islower(*ibuf))
X			btmp = (btmp << 6) | (26 + *ibuf - 'a');
X		else  /* In case of pad and error. */
X			btmp = (btmp << 6) | 0;
